#!/usr/bin/env python3
# -*- coding: utf-8 -*-
import os
import sys

if os.system("alpha2aleph --version") == 0:
    print("\nError !\n" \
          "alpha2aleph being already installed on this system, you CAN'T call bin/alpha2aleph; " \
          "please uninstall alpha2aleph before launching such a script; " \
          "\ntry :" \
          "\n  $sudo pip3 uninstall alpha2aleph")
    sys.exit(-999)

sys.path.append(os.path.join("..", "alpha2aleph"))

from alpha2aleph.main import entrypoint
entrypoint()
